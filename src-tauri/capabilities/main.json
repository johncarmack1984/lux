{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main-capability",
  "description": "Capability for the main window",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "allow-set-buffer",
    "allow-sync-channels",
    "allow-sync-state",
    "allow-update-channel-value",
    "app:default",
    "cli:default",
    "event:default",
    "http:default",
    "image:default",
    "log:default",
    "menu:default",
    "notification:default",
    "path:default",
    "resources:default",
    "sql:default",
    "tray:default",
    "webview:default",
    "window:default",
    "websocket:default",
    "http:default"
  ]
}
{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main-capability",
  "description": "Capability for the main window",
  "local": true,
  "windows": ["main"],
  "permissions": [
    "app:default",
    "cli:default",
    "event:default",
    "http:default",
    "log:default",
    "menu:default",
    "notification:default",
    "path:default",
    "resources:default",
    "tray:default",
    "window:default"
  ]
}

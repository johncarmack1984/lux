{
  "identifier": "mobile",
  "description": "Capability to build for mobile devices",
  "local": true,
  "platforms": ["android"],
  "windows": ["main"],
  "permissions": [
    "allow-set-buffer",
    "allow-sync-channels",
    "allow-sync-state",
    "allow-update-channel-value",
    "app:default",
    "barcode-scanner:allow-scan",
    "biometric:allow-authenticate",
    "cli:default",
    "event:default",
    "http:default",
    "image:default",
    "log:default",
    "menu:default",
    "nfc:allow-scan",
    "notification:default",
    "path:default",
    "resources:default",
    "sql:default",
    "tray:default",
    "webview:default",
    "window:default",
    "websocket:default"
  ]
}
